{
  "expo": {
    "name": "MyApp",
    "slug": "MyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coffee-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/coffee-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "mdpi": "./assets/images/coffee-splash.png",
      "hdpi": "./assets/images/coffee-splash.png",
      "xhdpi": "./assets/images/coffee-splash.png",
      "xxhdpi": "./assets/images/coffee-splash.png",
      "xxxhdpi": "./assets/images/coffee-splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/coffee-splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "tabletImage": "./assets/images/coffee-splash.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coffee-icon.png", // Используйте иконку здесь
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/coffee-splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "mdpi": "./assets/images/coffee-splash.png",
        "hdpi": "./assets/images/coffee-splash.png",
        "xhdpi": "./assets/images/coffee-splash.png",
        "xxhdpi": "./assets/images/coffee-splash.png",
        "xxxhdpi": "./assets/images/coffee-splash.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/coffee-icon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
